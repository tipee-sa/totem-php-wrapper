includes:
    - 'vendor/phpstan/phpstan/conf/bleedingEdge.neon'
    - 'phpstan-baseline.neon'

parameters:
    level: max
    tmpDir: 'var/cache/phpstan'
    paths:
        - 'lib'
        - 'test'
    reportUnmatchedIgnoredErrors: true
    checkFunctionNameCase: true
    checkUninitializedProperties: true
    tipsOfTheDay: false
